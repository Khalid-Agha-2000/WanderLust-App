
<% layout("layouts/boilerplate") -%>
<body>
    <h3>All Listings</h3>
    <form action="/listings/add" method="get">
        <button>Add Listing</button>
    </form>
    <ul>
        <% for(let listing of allListings) {%>
            <li><a href="/listings/<%= listing._id %>"><%= listing.title %></a></li>
        <%}%>
    </ul>
</body>